#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/前端/kawa-tiny-vue/node_modules/git-raw-commits/node_modules:/mnt/e/前端/kawa-tiny-vue/node_modules:/mnt/e/前端/node_modules:/mnt/e/node_modules:/mnt/e/前端/kawa-tiny-vue/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/前端/kawa-tiny-vue/node_modules/git-raw-commits/node_modules:/mnt/e/前端/kawa-tiny-vue/node_modules:/mnt/e/前端/node_modules:/mnt/e/node_modules:/mnt/e/前端/kawa-tiny-vue/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../git-raw-commits/cli.mjs" "$@"
else
  exec node  "$basedir/../git-raw-commits/cli.mjs" "$@"
fi
